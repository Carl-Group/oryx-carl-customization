{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch UI",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}/ui",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "start"],
            "env": {
                "NODE_ENV": "development"
            }
        },
        {
            "name": "Launch Platform",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/platform"
        }
    ]
}